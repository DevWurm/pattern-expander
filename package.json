{
  "name": "pattern-expander",
  "version": "1.0.0",
  "description": "Library to expand a pattern with permutations of specified values.",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "test": "mocha",
    "test:watch": "mocha -w",
    "test:cover": "istanbul cover _mocha",
    "pretest:coveralls": "npm run test:cover",
    "test:coveralls": "cat ./coverage/lcov.info | coveralls",
    "flow": "flow",
    "flow:stop": "flow stop",
    "prebuild": "npm run clean",
    "build": "babel src -d lib",
    "prebuild:dev": "npm run prebuild",
    "build:dev": "babel src -d lib --source-maps \"both\""
  },
  "repository": {
    "type": "git",
    "url": "https+git://github.com/DevWurm/pattern-expander.git"
  },
  "keywords": [
    "pattern",
    "glob",
    "expansion"
  ],
  "author": "DevWurm",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "coveralls": "^2.11.13",
    "flow-bin": "0.32.0",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "3.0.2"
  }
}
